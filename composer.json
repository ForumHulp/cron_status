{
	"name": "forumhulp/cron_status",
	"type": "phpbb-extension",
	"description": "Cron Status shows a notification on main page of acp panel when a cron job is locked. It shows the time when the cron started and which cron was the problem. In Maintenance Panel Cron Status display's a overview of the boards Cron Jobs",
	"homepage": "http://forumhulp.com",
	"version": "3.1.0",
	"keywords": ["forumhulp", "phpbb", "extension", "users"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "John Peskens",
			"username": "John P",
			"email": "info@forumhulp.com",
			"role": "Developer"
		},
		{
			"name": "Igor Lavrov",
			"homepage": "https://github.com/lavigor",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"phpbb/phpbb": "3.1.*@dev"
	},
	"extra": {
		"display-name": "Cron Status",
		"soft-require": {
           "phpbb/phpbb": "3.1.*@dev"
        },
		"version-check": {
			"host": "forumhulp.com",
			"directory": "/ext",
			"filename": "index.php?id=28"
		}
	}
}
